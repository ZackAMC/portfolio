---
import ItemMenu from "./ItemMenu.astro"
import IconHome from "@/icons/IconHome.astro"
import IconUser from "@/icons/IconUser.astro"
import IconStar from "@/icons/IconStar.astro"
import IconMail from "@/icons/IconMail.astro"
---

<ul class=" absolute bottom-10 left-1/2 z-[99] flex justify-center gap-3 translate-x-[-50%] p-2 bg-gradient-to-r from-[#FF8641] to-[#FF6126] rounded-full">

    <ItemMenu link="/home" >
        <IconHome />
    </ItemMenu>

    <ItemMenu link="/sobre-mi" >
        <IconUser />
    </ItemMenu>

    <ItemMenu link="/proyectos" >
        <IconStar />
    </ItemMenu>

    <ItemMenu link="/contacto" >
        <IconMail />
    </ItemMenu>

</ul>